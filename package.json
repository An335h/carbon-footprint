{
  "name": "carbon-footprint",
  "version": "1.1.0",
  "description": "Calculate your carbon footprint. Food, transport and purchases.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "failing-test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotMyFaultEarth/carbon-footprint.git"
  },
  "keywords": [
    "carbon",
    "footprint",
    "calculate",
    "food",
    "transport",
    "purchases"
  ],
  "author": "NMF.earth <contact@nmf.earth> (http://nmf.earth)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NotMyFaultEarth/carbon-footprint/issues"
  },
  "homepage": "https://github.com/NotMyFaultEarth/carbon-footprint#readme",
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/ramda": "0.26.39",
    "husky": "4.2.1",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ramda": "0.26.1",
    "ts-jest": "24.3.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5"
  },
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {}
}
